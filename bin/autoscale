
#JAVA="$JAVA_HOME"
JAVA=/usr/bin/java

AUTOSCALE_HOME=`dirname $0`/..

AUTOSCALE_CONF=$AUTOSCALE_HOME/src/main/resources

autoscale_bin=$AUTOSCALE_HOME/target/autoscale-1.0.0-SNAPSHOT.jar


CLASSPATH=$AUTOSCALE_CONF:$autoscale_bin

CLASSOATH=$CLASSPATH:$M2_REPO/org/slf4j/slf4j-log4j12/1.5.11/slf4j-log4j12-1.5.11.jar

# Classname
classname="org.uio.autoscale.service.AutoscaleDaemon"


exec "$JAVA" -cp "$CLASSPATH" "$classname"